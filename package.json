{
  "name": "vue-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode test",
    "build:test": "vue-cli-service build --mode test",
    "build:online": "vue-cli-service build --mode online",
    "build:prod": "vue-cli-service build --mode online",
    "lint": "vue-cli-service lint",
    "init": "node ./src/common/initTemplate/index.js"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.16",
    "qs": "^6.11.0",
    "vant": "^3.4.6",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.18.6",
    "@types/blueimp-md5": "^2.18.0",
    "@types/node": "^18.7.12",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "axios": "^0.26.1",
    "babel-plugin-import": "^1.13.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^8.0.3",
    "inquirer": "^8.2.4",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-px-to-viewport-8-plugin": "^1.1.3",
    "poststylus": "^1.0.1",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "style-resources-loader": "^1.5.0",
    "ts-import-plugin": "^2.0.0",
    "typescript": "~4.5.5",
    "vconsole-webpack-plugin": "^1.7.3",
    "vue-cli-plugin-style-resources-loader": "^0.1.5",
    "webpack": "^5.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "vue/setup-compiler-macros": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "@vue/standard",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "no-tabs": "off",
      "indent": "off",
      "space-before-function-paren": "off",
      "camelcase": "off",
      "no-mixed-spaces-and-tabs": "off",
      "@typescript-eslint/no-this-alias": "off",
      "@typescript-eslint/no-explicit-any": [
        "off"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
